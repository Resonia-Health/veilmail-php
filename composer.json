{
    "name": "veilmail/veilmail-php",
    "description": "Official PHP SDK for the Veil Mail API",
    "type": "library",
    "license": "MIT",
    "keywords": ["email", "veilmail", "api", "sdk"],
    "homepage": "https://veilmail.xyz",
    "support": {
        "issues": "https://github.com/Resonia-Health/veilmail-php/issues",
        "source": "https://github.com/Resonia-Health/veilmail-php"
    },
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "VeilMail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VeilMail\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
